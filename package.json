{
  name: multi-tenant-saas-dashboard,
  private: true,
  version: 0.1.0,
  packageManager: pnpm@9.7.1,
  workspaces: [
    apps/*,
    packages/*,
    services/*
  ],
  scripts: {
    build: turbo run build,
    dev: turbo run dev --parallel,
    lint: turbo run lint,
    test: turbo run test,
    typecheck: turbo run typecheck,
    prepare: husky install
  },
  devDependencies: {
    turbo: ^2.1.2,
    typescript: ^5.5.4,
    eslint: ^9.10.0,
    prettier: ^3.3.3,
    husky: ^9.1.6
  }
}
